    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.134",
    "@rushstack/eslint-patch": "^1.1.4",
